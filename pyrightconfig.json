{
    "include": [
        "backend"
    ],
    "exclude": [
        "**/__pycache__",
        "**/node_modules",
        "**/venv",
        "**/.venv"
    ],
    "executionEnvironments": [
        {
            "root": "backend",
            "pythonVersion": "3.10",
            "pythonPlatform": "Windows",
            "extraPaths": [
                "backend",
                "backend/app"
            ]
        }
    ],
    "typeCheckingMode": "basic",
    "reportMissingImports": true,
    "reportMissingTypeStubs": false,
    "pythonVersion": "3.10"
}

